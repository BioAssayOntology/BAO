		<rdfs:label rdf:datatype="&xsd;string">near-infrared imaging</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002516"/>
	</owl:Class>

	<!-- http://www.bioassayontology.org/bao#BAO_0002519 -->

	<owl:Class rdf:about="http://www.bioassayontology.org/bao#BAO_0002519">
		<rdfs:label rdf:datatype="&xsd;string">mid-infrared imaging</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002516"/>
	</owl:Class>

