		<rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0000648"/>
	</owl:Class>

	<!-- http://www.bioassayontology.org/bao#BAO_0000752 -->

	<owl:Class rdf:about="http://www.bioassayontology.org/bao#BAO_0000752">
