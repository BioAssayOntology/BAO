	<owl:Class rdf:about="http://www.bioassayontology.org/bao#BAO_0002523">
		<rdfs:label rdf:datatype="&xsd;string">atomic force microscopy</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0000459"/>
	</owl:Class>

