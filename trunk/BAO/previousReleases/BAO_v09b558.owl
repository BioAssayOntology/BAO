                                    <rdf:Description>
                                        <xsd:minExclusive rdf:datatype="&xsd;float">49.0</xsd:minExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bao;BAO_0000195"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="&xsd;float"/>
                                <owl:withRestrictions rdf:parseType="Collection">
