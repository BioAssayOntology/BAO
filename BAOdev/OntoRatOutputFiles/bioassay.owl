        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physicochemical assay that measures the maximum solubility of a molecule in a particular solvent (typically water or PBS).</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">504606</obo:IAO_0000119>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">solubility assay</rdfs:label>
    </owl:Class>
